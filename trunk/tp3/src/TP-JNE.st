Object subclass: #ApplicationForm	instanceVariableNames: 'code'	classVariableNames: ''	poolDictionaries: ''	category: 'TP-JNE'!!ApplicationForm methodsFor: 'as yet unclassified' stamp: 'MarianoSemelman 11/7/2011 20:49'!code	"Devuelve code"	|  |	^ code! !!ApplicationForm methodsFor: 'as yet unclassified' stamp: 'MarianoSemelman 11/7/2011 21:29'!code: aCode	"Asigna el codigo"	|  |	code := aCode.	! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ApplicationForm class	instanceVariableNames: 'instancias'!!ApplicationForm class methodsFor: 'as yet unclassified' stamp: 'MarianoSemelman 11/7/2011 21:36'!newWith: aCode	"Crea el objeto con code como mensaje"	| res |	res := ApplicationForm  new.	res code: aCode.	self instancias at: aCode put: res.	^ res! !!ApplicationForm class methodsFor: 'as yet unclassified' stamp: 'MarianoSemelman 11/7/2011 21:39'!withCode: aCode	"devuelva, si existe, el cuestionario de codigo indicado. Sino nil."	|  |	^ self instancias at: aCode.! !